<div class="singup">
    <h1>新規登録</h1>
    <div class="singup-form">
        <%= form_for(@user, url: singup_path) do |f| %>
         <%= render 'shared/error_messages' %>
         <div class="form">
        <p><%= f.label :name %>
        <%= f.text_field :name %></p>

        <p><%= f.label :email %>
        <%= f.email_field :email %></p>

        <p><%= f.label :password %>
        <%= f.password_field :password %></p>

        <p><%= f.label :password_confirmation, "Confirmation" %>
        <%= f.password_field :password_confirmation %></p>

        <p><%= f.label  :img %>
        <%= f.file_field :img %></p>
        

        <p><%= f.submit "新規登録", class: "btn btn-primary" %></p>
        <% end %> 
        </div>
    </div>
</div>